{
    "name": "awebdesign/core",
    "description": "The Laravel Lumen Framework for OpenCart projects",
    "keywords": ["framework", "laravel", "lumen", "opencart"],
    "license": "MIT",
    "type": "project",    
    "require": {
        "php": "5.6.4",
        "laravel/lumen-framework": ">=5.4.7",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": ">=5.7",
        "mockery/mockery": "~0.9"
    },
    "autoload": {        
        "psr-4": {
            "AwebCore\\App\\": "App/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6.4"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}